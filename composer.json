{
    "name": "hanan467/realtime-notifier",
    "description": "A simple package for real-time notifications using Pusher",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hanan467\\RealtimeNotifier\\": "src/"
        }
    },
    "autoload-dev": {
    "psr-4": {
        "Hanan467\\RealtimeNotifier\\Tests\\": "tests/"
    }
    },
    "authors": [
        {
            "name": "Hanan Ansar",
            "email": "hananansaresmael@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^11.42",
        "illuminate/contracts": "^11.42",
        "pusher/pusher-php-server": "^7.2",
        "illuminate/events": "^11.42",
        "illuminate/broadcasting": "^11.42"

    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "laravel/framework": "^11.43",
        "orchestra/testbench": "^9.10"
    },

    "extra": {
        "laravel": {
            "providers": [
                "Hanan467\\RealtimeNotifier\\Providers\\RealtimeNotifierServiceProvider"
            ]
        }
    }
}
